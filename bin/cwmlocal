#!/bin/bash

log=$HOME/.cwm-errors
app=$(basename $0)

opt=(
    "VBoxClient-all"
)

run_cmd() {
    local cmd=$1
    if [ -n "$(which $cmd)" ]; then
        $cmd >/dev/null 2>&1 || true
        echo "$(date) $app: run $cmd success." >>$log
    fi
}

main() {
    for i in "${opt[@]}"; do
        run_cmd $i &
    done
}

main
